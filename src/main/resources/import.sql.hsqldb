    create table address (
        id bigint generated by default as identity (start with 1),
        city varchar(255),
        country varchar(255),
        latitude double,
        longitude double,
        postalcode varchar(255),
        referenceNumber integer,
        street varchar(255),
        streetNumber integer,
        primary key (id)
    )

    create table cardata (
        id bigint generated by default as identity (start with 1),
        timestamp timestamp,
        licencePlate_id bigint,
        location_id bigint,
        speed_id bigint,
        video_id bigint,
        primary key (id)
    )

    create table entry (
