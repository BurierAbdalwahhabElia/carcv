        primary key (id)
    )

    create table mediaobject (
        id bigint generated by default as identity (start with 1),
        URL varchar(255),
        mediatype integer,
        primary key (id)
    )

    create table speed (
        id bigint generated by default as identity (start with 1),
        speed double,
        unit integer,
        primary key (id)
    )

